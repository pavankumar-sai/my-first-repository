pipeline {
    agent any 
    stages {
        stage('Stage 1') {
            steps {
                echo 'Hello world!' 
            }
        }
        stage('Stage 2') {
            steps {
                echo 'built!' 
            }
        }
        stage('Stage 3') {
            steps {
                echo 'tested!' 
            }
        }
        stage('Stage 5') {
            steps {
                echo 'deployed!' 
            }
        }
    }
}
